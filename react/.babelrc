{
  "presets": [
    "env",
    "react",
    "stage-1"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "emotion",
          {
            "hoist": true
          }
        ]
      ]
    },
    "development": {
      "plugins": [
        [
          "emotion",
          {
            "sourceMap": true,
            "autoLabel": true
          },
          "react-hot-loader/babel"
        ]
      ]
    }
  }
}
